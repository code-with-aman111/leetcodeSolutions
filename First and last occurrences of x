class Solution
{
    public:
    vector<int> find(int arr[], int n , int x )
    {    
        int first=-1;
        int last=-1;
        vector<int>pq;
        for(int i=0;i<n;i++){
             if(arr[i]==x){
              first=i;
              break;
          }  
    }
        for(int i=0;i<n;i++){
          if(arr[i]==x){
              last=i;
          }  
    }
    pq.push_back(first);
    pq.push_back(last);
    return pq;
    }
};
